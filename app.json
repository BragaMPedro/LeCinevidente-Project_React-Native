{
  "expo": {
    "name": "le-cinevidente",
    "slug": "le-cinevidente",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "assets/icon.png",
    "userInterfaceStyle": "light",
    "backgroundColor":"#3E3C42",
    "primaryColor": "#ac86ff",
    "splash": {
      "image": "assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3E3C42"
